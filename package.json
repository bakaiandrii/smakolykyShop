{
  "name": "smakolykyshop",
  "version": "1.0.0",
  "description": "rest api node js",
  "main": "index.js",
  "scripts": {
    "start:dev": "node -r dotenv/config --inspect=0.0.0.0:9227 ./dist/",
    "start": "rimraf dist && npm run lint && copyfiles -e src/email-templates/*.ts -f src/email-templates/*.* dist/email-templates/ && tsc-watch --outDir ./dist --onSuccess \"npm run start:dev\" --onFailure \"'echo Compilation Failed'\"",
    "build": "rimraf dist && tsc",
    "tsc": "rimraf dist && npm run lint && tsc --watch",
    "lint": "eslint --fix --ext .ts,.js src"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.3",
    "handlebars": "^4.7.7",
    "helmet": "^4.1.1",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.7",
    "nodemailer": "^6.4.11"
  },
  "devDependencies": {
    "@types/cors": "^2.8.7",
    "@types/bcrypt": "^3.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/express-rate-limit": "^5.1.0",
    "@types/handlebars": "^4.1.0",
    "@types/helmet": "^0.0.48",
    "@types/nodemailer": "^6.4.0",
    "@types/mongoose": "^5.7.36",
    "@types/joi": "^14.3.4",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.1",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "copyfiles": "^2.3.0",
    "rimraf": "^3.0.2",
    "eslint": "^7.10.0",
    "ts-node": "^9.0.0",
    "tsc-watch": "^4.2.9",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3",
    "morgan": "^1.10.0"
  },
  "author": "",
  "license": "ISC"
}
